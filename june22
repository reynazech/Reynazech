list1=[1,2,3,4,5,6,7]
list2=[5,8,4,3,2]
l=1
r=9
print(list1)
print(list2)
print("Write a Python program to sum all the items in a list.")
print(sum(list1))
print(sum(list2))
print("Write a Python program to get the largest number from a list.")
print (max(list2))
print("Write a Python program to count the number of elements in a list within a specified range: ")
print(len(list(x for x in list1 if l <= x <= r)))
print("Write a Python program to print the numbers of a specified list after removing even numbers from it.")
list1 = [x for x in list1 if x%2!=0]
print(list1)
print("Write a Python program to print a specified list after removing the 0th, 4th and 5th elements.")
list3 = ['mercury', 'venus', 'earth', 'mars', 'saturn', 'jupiter']
print(list3)
list3 = [x for (i,x) in enumerate(list3) if i not in (0,4,5)]
print(list3)
